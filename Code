                    <!-- LIKE BUTTON -->
                    <a class="liked specs" href="#" title="like this post">
                        {LikeButton}
                    </a>
                    
                    <!-- TAGS -->
                    {block:HasTags}
                    {block:IfShowTags}
                    {block:Tags}
                    <a class="specs" href="{TagURL}" title="tagged as: {Tag}">
                        # {Tag}
                    </a>
                    {/block:Tags}
                    {block:IfShowTags}
                    {block:IfNotShowTags}
                        {block:PermalinkPage}
                        {block:Tags}
                        <a class="specs" href="{TagURL}" title="tagged as: {Tag}">
                            # {Tag}
                        </a>
                        {/block:Tags}
                        {/block:PermalinkPage}
                    {/block:IfNotShowTags}
                    {/block:HasTags}
                </div>
                
                {block:PermalinkPage}
                {block:NoteCount}
                {block:PostNotes}
                {PostNotes}
                {/block:PostNotes}
                {/block:NoteCount}
                {/block:PermalinkPage}

            </article>
            {/block:Posts}
            
            <!-- RESPONSIVE SEARCHBAR -->
            <div id="resp-searchbar">
                <form action="/search" method="get" class="sfm" name="theform">
                    <input type="text" name="q" placeholder="Search Blog" class="query"/>
                    <button type="submit" title="search!">
                        <span class="th th-loupe-o" aria-hidden="true"></span>
                    </button>
                </form>
            </div>

            <!-- PAGINATION -->
            {block:Pagination}
            <div id="pagination">
                {block:PreviousPage}
                <a href="{PreviousPage}" class="pagilink" title="previous page">
                    <div class="pagi">
                        <span class="th th-chevron-left" aria-hidden="true"></span> 
                        previous
                    </div>
                </a>
                {/block:PreviousPage}
                
                <div id="pagi-text">
                    Page {CurrentPage} out of {TotalPages} Pages
                </div>
                
                {block:NextPage}
                <a href="{NextPage}" class="pagilink" title="next page">
                    <div class="pagi">
                        next <span class="th th-chevron-right" aria-hidden="true"></span>
                    </div>
                </a>
                {/block:NextPage}
            </div> <!-- // end pagination -->
            {/block:Pagination}
            </div> <!-- // end posts -->
        </div>

